datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  userId    Int      @id @default(autoincrement())
  id        Int
  title     String
  body      String
  image     String
  createdAt DateTime @default(now())
}

model Post {
  userId    Int      @id @default(autoincrement())
  id        Int
  title     String
  body      String
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
